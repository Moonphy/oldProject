#popup-invite.popup_window
    .header 邀请朋友来爱窝网
    .content: input.input(type='text',placeholder='输入ta的邮箱点击发送即可')
    .button 发送邀请

#popup-gather.popup_window
    .title 转采
    .name
        img(src='images/examples/icon_websit.png')
        span 爱窝网
    .url www.iwoor.com
    .select 导航类网站
    .detail
        p.
            这是一个中国最热的设计社交类网站，很
            在这里集合了中国大部分设计师...
        ul.tags
            each item in ['平面设计','广告设计','电商','彩票','直播']
                li: a(href='search_website.html',target='_blank')= item
        .input
            span.label 标签
            input(type='text',placeholder='输入标签内容')
            .icon_delete
            span.btn-add 添加

    .btn-confirm-gather 采下来

    .panel-search
        .main
            ul
                each item in ['平面设计','广告设计','电商','彩票','直播','平面设计','广告设计','电商','彩票','直播']
                    li
                        .letter
                        .text= item

        input.search-input(placeholder='快速筛选/创建小窝')



#popup-share.popup_window
    .title 分享网址
    .row1
        .label 网站名称
        .name: input(type='text',placeholder='如：爱窝网')
        .label 网站地址
        .address: input(type='text',placeholder='如：www.iwoor.com')
    .row2
        .label 介绍
        .instroduce: textarea(placeholder='输入一段话介绍一下这个网站吧！')
    .row3
        .label 标签
        .tag 收藏网站
        .tag 网址
        .tag 网址大全
        .tag-input: input(type='text',placeholder='输入标签内容')
        .button 添加
    .btn-confirm-to-next 下一步