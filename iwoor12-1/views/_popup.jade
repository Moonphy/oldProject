#popup-new-folder.popup_window
    .header 创建小窝
    .row1
        .label 小窝名称
        input.input(type='text')
    .row2
        .label 小窝分类
        input.select(type='text')
        #popup-edit.popup-edit
            ul
                li 科学
                li 开发
                li 平面设计
                li 汽车
                li 美女
                li 图片素材
                li 在线教育
                li 美食
                li 站长
                li 编辑
    .button 确认创建

#popup-new-question.popup_window
    .header 提问
    .content
        .row1
            input.input(type='text',placeholder='写下你的问题')
        .row1
            textarea(placeholder='问题说明')
        .row2
            .label 选择分类
            input.select(type='text',placeholder='导航类网站')
            #popup-edit.popup-edit
                ul
                    li 科学
                    li 开发
                    li 平面设计
                    li 汽车
                    li 美女
                    li 图片素材
                    li 在线教育
                    li 美食
                    li 站长
                    li 编辑
        .row3
            .title 标签
            .tag 收藏网站
            .tag 网址
            .tag 网址大全
            .tag-input: input(type='text',placeholder='输入标签内容')
            .add-tag 添加
        .button 提问

