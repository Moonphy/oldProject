
/*selectarea*/
.ui-dialog
  box-sizing: border-box

.ui-scroller
  position: relative
  height: 125px
  width: 100%
  min-width: 260px
  font-size: 14px
  font-weight: normal
  z-index 999

.ui-scroller>div
  width: 33%
  display: inline-block
  position: relative
  height: 125px
  overflow: hidden
  box-sizing: border-box
  z-index 0!important

.ui-scroller dl
  position: absolute
  width: 100%
  margin: 0
  top: 30px

.ui-scroller dd
  padding: 0
  margin: 0
  overflow: hidden
  text-overflow: ellipsis
  width: 100%
  height: 30px
  line-height: 30px

.ui-scroller p
  position: absolute
  top: 27px
  height: 30px
  width: 100%
  margin: 0
  z-index: 1!important
  border-top: 2px solid #5aacff!important
  border-bottom: 2px solid #5aacff!important
  border-left: none
  border-right: none

.ui-scroller .ui-dialog-action
  margin-top: 10px

