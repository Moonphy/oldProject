// 元件：面包屑导航
// ==================================================

.breadcrumb
  position relative
  remove-list-style()
  margin-bottom 0
  padding 10px 0
  clearfix()

  li
    float left
    color gray

  a
    color gray

    &:hover
      color link-color-hover
      text-decoration none

  .divider
    margin 0 5px
    color gray
